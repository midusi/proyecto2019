{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "10.6"
        }
      }
    ],
    "@babel/preset-react",
    "@babel/preset-env",
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "react-hot-loader/babel",
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "src": "./src",
      }
    }]
  ]
}

{
  "name": "react-redux-webpack-starter",
  "version": "1.0.0",
  "description": "A basic template that consists of the essential elements that are required to start building a React application using Redux",
  "scripts": {
    "build": "webpack",
    "build:dev": "yarn build --mode development",
    "build:dev:watch": "yarn build:dev --watch",
    "build:dev:bundleanalyze": "yarn build:dev --addons=bundleanalyze",
    "build:dev:bundlebuddy": "yarn build:dev --addons=bundlebuddy",
    "build:prod": "yarn build -p",
    "build:prod:watch": "yarn build:prod --watch",
    "build:prod:bundleanalyze": "yarn build:prod --addons=bundleanalyze",
    "build:prod:bundlebuddy": "yarn build:prod --addons=bundlebuddy",
    "upgrade:package": "yarn upgrade-interactive --latest",
    "lint": "eslint .; exit 0",
    "lint:fix": "eslint . --fix; exit 0",
    "reducer:generate": "reducer-maker -w src",
    "reducer:help": "reducer-maker --help",
    "serve:dev": "webpack-dev-server --mode development",
    "serve:dev:dashboard": "webpack-dashboard webpack-dev-server -- --mode development --addons=dashboard",
    "serve:prod": "yarn build:prod && live-server ./dist",
    "start": "yarn serve:dev",
    "test": "jest --config .jest.config.js",
    "test:ci": "yarn test --ci",
    "test:watch": "jest --config .jest.config.js --watch",
    "webpack-defaults": "webpack-defaults",
    "docker:dev": "yarn docker:build && yarn docker:start",
    "docker:build": "docker build -f docker/Dockerfile --target development -t midusi_proyecto_2019 .",
    "docker:start": "docker run --rm -it --network host -v $PWD:/usr/src/app midusi_proyecto_2019",
    "docker:prod": "yarn docker:build:prod && yarn docker:start:prod",
    "docker:build:prod": "docker build -f docker/Dockerfile --target production -t midusi_proyecto_2019:production .",
    "docker:start:prod": "docker run --rm -it --network host -v $PWD:/usr/src/app midusi_proyecto_2019:production"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "chart.js": "^2.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "face-api.js": "^0.20.1",
    "html-webpack-plugin": "^3.2.0",
    "i18next": "^17.0.1",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^3.0.0",
    "inject-html-webpack-plugin": "^0.0.39",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-chartkick": "^0.3.2",
    "react-count-to": "^0.12.0",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.9.0",
    "react-i18next": "7.9.0",
    "react-mixin": "^5.0.0",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-semantic-toasts": "^0.5.4",
    "react-slick": "^0.25.2",
    "react-timer-mixin": "^0.13.4",
    "react-webcam": "^1.1.1",
    "redux": "^4.0.1",
    "redux-cookie": "^0.5.9",
    "redux-devtools": "^3.5.0",
    "redux-devtools-chart-monitor": "^1.7.0",
    "redux-devtools-dispatch": "^2.2.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-filterable-log-monitor": "^0.8.1",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-devtools-multiple-monitors": "^1.0.1",
    "redux-form": "^8.2.3",
    "redux-logger": "^3.0.6",
    "redux-slider-monitor": "^2.0.0-2",
    "redux-thunk": "^2.3.0",
    "semantic-ui-calendar-react": "^0.15.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-less": "^2.4.1",
    "semantic-ui-react": "^0.87.3",
    "slick-carousel": "^1.8.1",
    "terser-webpack-plugin": "^1.3.0",
    "underscore": "^1.9.1",
    "validate.js": "^0.13.1",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dashboard": "^3.0.7",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/moment-timezone": "^0.5.12",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.12.0",
    "reducer-maker": "^0.0.8",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack-dev-server": "^3.6.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
